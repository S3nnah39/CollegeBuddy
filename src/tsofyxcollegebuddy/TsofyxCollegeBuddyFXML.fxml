<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="700.0" styleClass="mainFxmlClass" stylesheets="@tsofyxcollegebuddystyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tsofyxcollegebuddy.TsofyxCollegeBuddyController">
   <children>
      <VBox fx:id="sideTheme" layoutY="96.0" prefHeight="354.0" prefWidth="139.0" styleClass="gradient" stylesheets="@tsofyxcollegebuddystyle.css">
         <children>
            <Label fx:id="dashboard" alignment="CENTER" onMouseClicked="#loadDashboard" prefHeight="26.0" prefWidth="140.0" style="-fx-border-width: 2px 0;" text="Dashboard" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="performance" alignment="CENTER" onMouseClicked="#loadPerformance" prefHeight="31.0" prefWidth="140.0" text="Performance" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="graduationPlan" alignment="CENTER" onMouseClicked="#loadGraduationPlan" prefHeight="26.0" prefWidth="138.0" text="Graduation Plan" textFill="#eeeeee">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="budget" alignment="CENTER" layoutX="10.0" layoutY="237.0" onMouseClicked="#loadBudgetPage" prefHeight="26.0" prefWidth="141.0" text="Budget" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="signOut" mnemonicParsing="false" onAction="#signUserOut" style="-fx-background-color: none; -fx-border-color: white;" text="Sign Out" textFill="WHITE">
               <VBox.margin>
                  <Insets left="60.0" top="60.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <HBox fx:id="headerTheme" prefHeight="63.0" prefWidth="700.0" styleClass="gradient" stylesheets="@tsofyxcollegebuddystyle.css">
         <children>
            <ImageView fitHeight="40.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/userPic.png" />
               </image>
               <HBox.margin>
                  <Insets left="15.0" top="10.0" />
               </HBox.margin>
            </ImageView>
            <Text fx:id="userName" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Username">
               <font>
                  <Font size="13.0" />
               </font>
               <HBox.margin>
                  <Insets left="15.0" top="18.0" />
               </HBox.margin>
            </Text>
            <Button fx:id="themeBtn" mnemonicParsing="false" onAction="#changeTheme" style="-fx-background-radius: 2%;" styleClass="shadow" text="Theme" textFill="#f5f5f5">
               <HBox.margin>
                  <Insets left="480.0" top="15.0" />
               </HBox.margin>
            </Button>
         </children></HBox>
      <Label fx:id="about" alignment="CENTER" layoutX="1.0" layoutY="66.0" onMouseClicked="#loadAbout" prefHeight="26.0" prefWidth="138.0" text="About">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
